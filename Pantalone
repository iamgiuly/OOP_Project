package abbigliamento;

/**
 *
 * @author Giulia Evangelisti
 */
public class Pantalone extends Abbigliamento {
    String model;
    
public void setMod (String mod){
    this.model=mod;
}

public void geMod(){
    System.out.println(this.model);
}

public Pantalone()
{ 
    this.genere="";
    this.materiale="";
    this.NrPers=0;
    this.Pers=false; 
    this.prezzobase=0;
    this.colore="Bianco";
    this.Tipostampa="notdefined";
    this.model="notdefined";
    this.PrezzoUnitario=0;
    
}

public Pantalone(String g, String m, int nrpers, boolean p, float pb, String s, String tipost, String mod, float pu)
{ 
    this.genere=g;
    this.materiale=m;
    this.NrPers=nrpers;
    this.Pers=p; 
    this.prezzobase=pb;
    this.colore=s;
    this.Tipostampa=tipost;
    this.model=mod;
    this.PrezzoUnitario=pu;
}

}
