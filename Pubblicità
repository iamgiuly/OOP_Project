package Pubblicità;
import merce.*;

/**
 *
 * @author Sara
 */
public class Pubblicità extends Merce {
    String TipoCarta;
    int spessore; String formato;
    int IDPubb;
    public void getTipoCarta()
    {
        System.out.println(this.TipoCarta);
    }
    public void setTipoCarta(String tc)
    {
        this.TipoCarta=tc;
    }
    public void getSpessore()
    {
        System.out.println(this.spessore);
    }
    public void setSpessore(int sp)
    {
        this.spessore=sp;
    }
    public void getFormato()
    {
        System.out.println(this.formato);
    }
    public void setFormato(String form)
    {
        this.formato=form;
    }
    public void getIDPubb()
    {
        System.out.println(this.IDPubb);
    }
    public void setIDPubb(int ID)
    {
        this.IDPubb=ID;
    }
    public Pubblicità()
    {
        TipoCarta="";
        spessore=0;
        formato="";
        IDPubb=0;
    }
    public Pubblicità(String tc,int sp,String form,int ID)
    {
        TipoCarta=tc;
        spessore=sp;
        formato=form;
        IDPubb=ID;  
    }
}
