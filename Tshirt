package abbigliamento;

/**
 *
 * @author Sara
 */
public class Tshirt extends Abbigliamento {
    float prezzobase;
    public void setPrezzoBase(float pb)
    {
        prezzobase=pb;
    }
    public void getPrezzoBase()
    {
        System.out.println(prezzobase);
    }
    public Tshirt()
    {
        prezzobase=0;
    }
    public Tshirt(int pb)
    {
        prezzobase=pb;
    }
}
